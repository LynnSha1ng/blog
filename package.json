{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-s \"gen -d\" dev-only",
    "build:local": "run-s type-check gen:run \"build-only {@}\" --",
    "build:workflow": "run-s type-check build-only",
    "dev-only": "vite",
    "build-only": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "gen:build": "tsc -p tsconfig.gen.json",
    "gen:run": "node scripts/dist/gen/run.js",
    "gen": "run-s gen:build \"gen:run {1}\" --",
    "lint": "eslint . --fix",
    "format": "prettier --write src/ scripts/"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.2",
    "@vueuse/core": "^12.0.0",
    "dexie": "^4.0.10",
    "highlight.js": "^11.10.0",
    "marked": "^15.0.3",
    "marked-emoji": "^1.4.3",
    "marked-highlight": "^2.2.1",
    "pinia": "^2.2.8",
    "sass": "^1.81.0",
    "typescript": "~5.6.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.17.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.16.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.4.1",
    "vite": "^6.0.1",
    "vite-plugin-static-copy": "^2.2.0",
    "vue-tsc": "^2.1.10",
    "yaml": "^2.6.1"
  }
}
