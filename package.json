{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-s gen dev-only",
    "build:local": "run-s type-check gen:run \"build-only {@}\" --",
    "build:workflow": "run-s type-check build-only",
    "dev-only": "vite",
    "build-only": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "gen:build": "tsc -p tsconfig.gen.json",
    "gen:run": "node --env-file=.env scripts/dist",
    "gen": "run-s gen:build gen:run",
    "lint": "eslint . --fix",
    "format": "prettier --write src/ scripts/"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.2",
    "@vueuse/core": "^11.1.0",
    "dexie": "^4.0.8",
    "highlight.js": "^11.10.0",
    "marked": "^14.1.2",
    "marked-emoji": "^1.4.2",
    "marked-highlight": "^2.1.4",
    "pinia": "^2.2.4",
    "sass": "^1.79.5",
    "vue": "^3.5.11",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.16.9",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.29.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "typescript": "~5.5.0",
    "vite": "^5.4.8",
    "vite-plugin-static-copy": "^2.0.0",
    "vue-tsc": "^2.1.6",
    "yaml": "^2.5.1"
  }
}
